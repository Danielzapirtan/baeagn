#! /bin/bash

BN=$1

set -e

clear
git add .
git commit -m yaccg || true
git push || true
gh workflow run bench$BN
sleep 50
export wfid=$(gh run list --workflow bench$BN |head -n 1 |cut -f 7)
export jid=$(gh run view $wfid|grep -o "ID.*)"|grep -o "[[:digit:]]\+")
sleep $2
gh run view --log --job $jid|grep -i player
gh run view --log --job $jid|grep -i valuation|tail -n 1
gh run view --log --job $jid|grep -i variation|tail -n 1


